{
	"name": "zabbuntu-frontend",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "vite build && s3-deploy ./build/**/* --cwd ./build --region us-east-1 --bucket zabbuntu.carlosmoshu.com --delete-removed --cache-control 'max-age=31536000, public' --acl public-read",
		"help": "vite deploy"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "4.5.10",
		"@neoconfetti/svelte": "1.0.0",
		"@sveltejs/adapter-auto": "1.0.0",
		"@sveltejs/kit": "1.0.0",
		"@types/bootstrap": "5.2.6",
		"@types/cookie": "0.5.1",
		"@typescript-eslint/eslint-plugin": "5.45.0",
		"@typescript-eslint/parser": "5.45.0",
		"axios": "1.2.2",
		"bootstrap": "5.2.3",
		"dotenv": "16.0.3",
		"eslint": "8.28.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "2.8.0",
		"prettier-plugin-svelte": "2.8.1",
		"svelte": "3.54.0",
		"svelte-check": "2.9.2",
		"tslib": "2.4.1",
		"typescript": "4.9.3",
		"vite": "4.0.3"
	},
	"type": "module",
	"author": {
		"name": "Carlos Moshu",
		"email": "carlos@carlosmoshu.com",
		"url": "https://carlosmoshu.com"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/carlosmoshudev/Zabbuntu-Frontend.git"
	}
}
